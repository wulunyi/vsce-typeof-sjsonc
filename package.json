{
	"name": "vsce-typeof-sjsonc",
	"displayName": "typeof-sjsonc",
	"description": "typeof-sjsonc description",
	"version": "0.0.3",
	"publisher": "wulunyi",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.typeof-sjsonc"
	],
	"main": "./out/extension.js",
	"repository": "https://github.com/wulunyi/vsce-typeof-sjsonc.git",
	"contributes": {
		"commands": [
			{
				"command": "extension.typeof-sjsonc",
				"title": "typeof from clipboard"
			}
		],
		"keybindings": [
			{
				"command": "extension.typeof-sjsonc",
                "key": "ctrl+alt+V",
                "args": {}
            },
            {
				"command": "extension.typeof-sjsonc",
				"key": "ctrl+alt+S",
				"args": {"separate": true}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "rollup -c ./rollup.config.js",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"pack": "vsce package"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"@rollup/plugin-typescript": "^3.0.0",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.17.16",
		"rollup": "^1.31.1",
		"rollup-plugin-cleanup": "^3.1.1",
		"rollup-plugin-delete": "^1.2.0",
		"rollup-plugin-terser": "^5.2.0",
		"rollup-plugin-visualizer": "^3.3.1",
		"tslint": "^5.20.1",
		"typescript": "^3.8.2",
		"vsce": "^1.73.0",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"tslib": "^1.11.0",
		"typeof-sjsonc": "^1.0.2-beta.1"
	}
}
