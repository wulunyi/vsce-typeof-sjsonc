{
	"name": "typeof-sjsonc",
	"displayName": "typeof-sjsonc",
	"description": "typeof-sjsonc description",
	"version": "0.0.1",
	"publisher": "wulunyi",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.typeof-sjsonc"
	],
	"main": "./out/extension.js",
	"repository": "https://github.com/wulunyi/vsce-typeof-jsonc.git",
	"contributes": {
		"commands": [
			{
				"command": "extension.typeof-sjsonc",
				"title": "typeof from clipboard"
			}
		],
		"keybindings": [
			{
				"command": "extension.typeof-sjsonc",
				"key": "ctrl+alt+V"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "rollup -c ./rollup.config.js",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"pack": "vsce package"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^11.0.1",
		"@rollup/plugin-node-resolve": "^7.0.0",
		"@rollup/plugin-typescript": "^3.0.0",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"rollup": "^1.30.1",
		"rollup-plugin-cleanup": "^3.1.1",
		"rollup-plugin-terser": "^5.2.0",
		"rollup-plugin-visualizer": "^3.3.1",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vsce": "^1.62.0",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"tslib": "^1.10.0"
	}
}
